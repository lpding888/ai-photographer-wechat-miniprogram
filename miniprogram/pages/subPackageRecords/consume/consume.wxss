.container{background:#E9E3DF;min-height:100vh;padding:24rpx}
.header{padding:28rpx 20rpx}
.title{font-size:36rpx;font-weight:700;color:#000}
.empty{text-align:center;color:#666;margin:40rpx 0}
.list{display:flex;flex-direction:column;gap:16rpx}
.item{background:#fff;border-radius:16rpx;padding:20rpx 24rpx;box-shadow:0 4rpx 12rpx rgba(0,0,0,0.05)}
.row{display:flex;justify-content:space-between;padding:10rpx 0;border-bottom:1rpx solid #f3f3f3}
.row:last-child{border-bottom:none}
.name{color:#465C88;font-size:26rpx}
.value{color:#000;font-size:26rpx}