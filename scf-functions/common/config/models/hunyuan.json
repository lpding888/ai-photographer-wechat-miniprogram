{
  "name": "混元大模型",
  "type": "hunyuan",
  "version": "1.0.0",
  "description": "腾讯云混元大模型配置 - 专注图像分析",
  "envVars": {
    "secretId": "TENCENTCLOUD_SECRET_ID",
    "secretKey": "TENCENTCLOUD_SECRET_KEY",
    "envId": "TCB_ENV_ID"
  },
  "useCloudBase": true,
  "envId": "your-env-id-here",
  "region": "ap-beijing",
  "defaultModel": "hunyuan-vision",
  "defaultParams": {
    "temperature": 0.3,
    "maxTokens": 2000,
    "timeout": 30000
  },
  "capabilities": [
    "image_analysis",
    "text_generation",
    "multi_modal"
  ],
  "pricing": {
    "input_tokens": 0.000003,
    "output_tokens": 0.000006,
    "currency": "CNY"
  },
  "limits": {
    "max_images_per_request": 5,
    "max_prompt_length": 10000,
    "max_response_length": 2000,
    "rate_limit_per_minute": 60
  },
  "features": {
    "vision_analysis": true,
    "multi_image_support": true,
    "json_output": true,
    "chinese_support": true
  },
  "recommendations": {
    "best_for": "image_analysis",
    "use_cases": [
      "服装照片分析",
      "人物特征识别",
      "风格判断",
      "场景理解"
    ],
    "prompt_tips": [
      "使用清晰的分析指令",
      "要求JSON格式输出",
      "指定分析维度（人物、服装、姿势、风格）"
    ]
  },
  "fallback": {
    "enabled": true,
    "backup_analysis": {
      "person": {
        "age": "未知",
        "gender": "未知",
        "hair": "未知",
        "bodyType": "中等",
        "expression": "自然"
      },
      "clothing": {
        "type": "服装",
        "color": "多色",
        "style": "时尚",
        "material": "未知"
      },
      "pose": {
        "posture": "站立",
        "action": "自然",
        "angle": "正面"
      },
      "style": {
        "overall": "时尚",
        "mood": "自然",
        "occasion": "日常"
      }
    }
  }
}