{
  "name": "豆包Seedream 4.0",
  "type": "doubao",
  "version": "4.0.0",
  "description": "字节跳动豆包图像生成模型配置 - 专注高质量图像生成",
  "envVars": {
    "apiKey": "DOUBAO_API_KEY"
  },
  "apiEndpoint": "https://ark.cn-beijing.volces.com/api/v3",
  "apiKey": "your-doubao-api-key-here",
  "defaultModel": "doubao-Seedream-4-0-250828",
  "defaultParams": {
    "size": "2K",
    "quality": "standard",
    "watermark": false,
    "maxImages": 4,
    "timeout": 300000
  },
  "supportedSizes": ["512x512", "1K", "2K", "4K"],
  "supportedQualities": ["standard", "hd"],
  "supportedStyles": [
    "realistic",
    "anime",
    "oil_painting",
    "watercolor",
    "sketch",
    "cartoon",
    "cyberpunk",
    "vintage",
    "fantasy",
    "minimalist"
  ],
  "capabilities": [
    "image_generation",
    "image_variation",
    "multi_fusion",
    "reference_generation",
    "sequential_generation"
  ],
  "features": {
    "4k_support": true,
    "multi_image_fusion": true,
    "reference_generation": true,
    "style_transfer": true,
    "sequential_generation": true,
    "custom_params": true
  },
  "pricing": {
    "per_image": {
      "512x512": 0.001,
      "1K": 0.002,
      "2K": 0.008,
      "4K": 0.032
    },
    "currency": "CNY"
  },
  "limits": {
    "max_images_per_request": 6,
    "max_reference_images": 5,
    "max_prompt_length": 1000,
    "max_concurrent_requests": 5,
    "rate_limit_per_minute": 30
  },
  "presets": {
    "portrait": {
      "size": "2K",
      "quality": "hd",
      "maxImages": 2,
      "style": "realistic",
      "enableSequential": false
    },
    "fashion": {
      "size": "2K",
      "quality": "hd",
      "maxImages": 4,
      "style": "realistic",
      "enableSequential": true
    },
    "artistic": {
      "size": "1K",
      "quality": "standard",
      "maxImages": 6,
      "enableSequential": true
    },
    "quick": {
      "size": "512x512",
      "quality": "standard",
      "maxImages": 1,
      "enableSequential": false
    }
  },
  "recommendations": {
    "best_for": "image_generation",
    "use_cases": [
      "服装摄影生成",
      "模特图像创建",
      "风格迁移",
      "创意设计"
    ],
    "prompt_tips": [
      "详细描述人物特征和服装",
      "指定风格和场景",
      "包含光线和氛围要求",
      "使用质量相关的关键词"
    ]
  },
  "integration": {
    "compatible_with": ["hunyuan"],
    "workflow": "hunyuan_analysis -> doubao_generation",
    "data_format": {
      "input": "text_prompt + optional_reference_images",
      "output": "image_urls + metadata"
    }
  }
}